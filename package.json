{
  "name": "hack-ed",
  "version": "0.0.1",
  "author": "Chumlung",
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.6.3",
    "jquery": "^1.11.3",
    "prettier": "^1.19.1",
    "uglify-js": "^3.9.4",
    "uglifycss": "0.0.29"
  },
  "scripts": {
    "prettier:fix": "prettier --write './**/*.{js,html,scss}'",
    "uglifyjs": "touch ./js/hacked.min.js && uglifyjs ./js/hacked.js -m -c -o ./js/hacked.min.js",
    "uglifycss": "touch ./css/hacked.min.css && uglifycss ./css/hacked.css > ./css/hacked.min.css",
    "build": "npm run uglifyjs && npm run uglifycss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackrockDigital/startbootstrap-grayscale.git"
  }
}
